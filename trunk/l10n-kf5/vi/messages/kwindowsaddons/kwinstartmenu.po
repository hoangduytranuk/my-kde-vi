#: main.cpp:39
#, kde-format
msgid "kwinstartmenu"
msgstr "kwinstartmenu"

#: main.cpp:40
#, kde-format
msgid "An application to create/update or remove Windows Start Menu entries"
msgstr ""
"Một ứng dụng để tạo/cập nhật hay xoá các mục Trình đơn Bắt đầu của Windows"

#: main.cpp:42
#, kde-format
msgid "(C) 2008-2011 Ralf Habacker"
msgstr "(C) 2008-2011 Ralf Habacker"

#: main.cpp:46
#, kde-format
msgid "remove installed start menu entries"
msgstr "xoá các mục trình đơn Bắt đầu đã được cài"

#: main.cpp:47
#, kde-format
msgid "install start menu entries"
msgstr "cài đặt các mục trình đơn Bắt đầu"

#: main.cpp:48
#, kde-format
msgid "update start menu entries"
msgstr "cập nhật các mục trình đơn Bắt đầu"

#: main.cpp:49
#, kde-format
msgid "remove start menu entries from unused kde installation"
msgstr "xoá các mục trình đơn Bắt đầu khỏi bản cài đặt KDE không dùng đến"

#: main.cpp:51
#, kde-format
msgid "query root path of start menu entries"
msgstr "truy vấn đường dẫn gốc của các mục trình đơn Bắt đầu"

#: main.cpp:53
#, kde-format
msgid "use categories for start menu entries (default)"
msgstr "dùng các phân nhóm cho các mục trình đơn Bắt đầu (mặc định)"

#: main.cpp:54
#, kde-format
msgid "don't use categories for start menu entries"
msgstr "không dùng các phân nhóm cho các mục trình đơn Bắt đầu"

#: main.cpp:55
#, kde-format
msgid "query current value of categories in start menu"
msgstr "truy vấn giá trị hiện tại của các phân nhóm trong trình đơn Bắt đầu"

#: main.cpp:57
#, kde-format
msgid "set custom string for root start menu entry"
msgstr "đặt chuỗi tuỳ biến cho mục trình đơn Bắt đầu gốc"

#: main.cpp:60
#, kde-format
msgid "remove custom string from root start menu entry"
msgstr "xoá chuỗi tuỳ biến khỏi mục trình đơn Bắt đầu gốc"

#: main.cpp:61
#, kde-format
msgid "query current value of root start menu entry custom string"
msgstr "truy vấn giá trị hiện tại của chuỗi tuỳ biến trình đơn Bắt đầu gốc"

#: main.cpp:63
#, kde-format
msgid "set custom name string for root start menu entry"
msgstr "đặt chuỗi tên tuỳ biến cho mục trình đơn Bắt đầu gốc"

#: main.cpp:64
#, kde-format
msgid "remove custom name string from root start menu entry"
msgstr "xoá chuỗi tên tuỳ biến cho mục trình đơn Bắt đầu gốc"

#: main.cpp:65
#, kde-format
msgid "query current value of start menu entry custom name string"
msgstr "truy vấn giá trị hiện tại của chuỗi tên tuỳ biến mục trình đơn Bắt đầu"

#: main.cpp:67
#, kde-format
msgid "set custom version string for root start menu entry"
msgstr "đặt chuỗi phiên bản tuỳ biến cho mục trình đơn Bắt đầu gốc"

#: main.cpp:68
#, kde-format
msgid "remove custom version string from root start menu entry"
msgstr "xoá chuỗi phiên bản tuỳ biến khỏi mục trình đơn Bắt đầu gốc"

#: main.cpp:69
#, kde-format
msgid "query current value of root start menu entry version string"
msgstr ""
"truy vấn giá trị hiện tại của chuỗi phiên bản mục trình đơn Bắt đầu gốc"
