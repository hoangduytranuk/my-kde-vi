#: src/effects/backgroundcontrast/metadata.json
msgctxt "Description"
msgid "Improve contrast and readability behind semi-transparent windows"
msgstr ""
"Cải thiện độ tương phản và tính dễ đọc phía sau các cửa sổ bán trong suốt"

#: src/effects/blur/metadata.json
msgctxt "Description"
msgid "Blurs the background behind semi-transparent windows"
msgstr "Làm mờ phần nền phía sau các cửa sổ bán trong suốt"

#: src/effects/colorpicker/metadata.json
msgctxt "Description"
msgid "Supports picking a color"
msgstr "Hỗ trợ nhặt màu"

#: src/effects/desktopgrid/metadata.json
msgctxt "Description"
msgid "Zoom out so all desktops are displayed side-by-side in a grid"
msgstr ""
"Thu nhỏ để tất cả các bàn làm việc được hiển thị cạnh nhau trong một lưới"

#: src/effects/diminactive/metadata.json
msgctxt "Description"
msgid "Darken inactive windows"
msgstr "Làm tối các cửa sổ bất hoạt"

#: src/effects/fallapart/metadata.json
msgctxt "Description"
msgid "Closed windows fall into pieces"
msgstr "Cửa sổ bị đóng sẽ vỡ thành nhiều mảnh"

#: src/effects/glide/metadata.json
msgctxt "Description"
msgid "Glide windows as they appear or disappear"
msgstr "Lướt các cửa sổ khi chúng xuất hiện hoặc biến mất"

#: src/effects/highlightwindow/metadata.json
msgctxt "Description"
msgid "Highlight the appropriate window when hovering over taskbar entries"
msgstr "Tô sáng cửa sổ thích hợp khi đảo chuột trên các mục thanh tác vụ"

#: src/effects/invert/metadata.json
msgctxt "Description"
msgid "Inverts the color of the desktop and windows"
msgstr "Đảo màu bàn làm việc và các cửa sổ"

#: src/effects/kscreen/metadata.json
msgctxt "Description"
msgid "Helper Effect for KScreen"
msgstr "Hiệu ứng trợ giúp cho KScreen"

#: src/effects/lookingglass/metadata.json
msgctxt "Description"
msgid "A screen magnifier that looks like a fisheye lens"
msgstr "Một trình phóng đại màn hình trông như một thấu kính mắt cá"

#: src/effects/magiclamp/metadata.json
msgctxt "Description"
msgid "Simulate a magic lamp when minimizing windows"
msgstr "Mô phỏng một cây đèn thần khi thu nhỏ các cửa sổ"

#: src/effects/magnifier/metadata.json
msgctxt "Description"
msgid "Magnify the section of the screen that is near the mouse cursor"
msgstr "Phóng đại khu vực màn hình gần với con trỏ chuột"

#: src/effects/mouseclick/metadata.json
msgctxt "Description"
msgid ""
"Creates an animation whenever a mouse button is clicked. This is useful for "
"screenrecordings/presentations"
msgstr ""
"Tạo một hiệu ứng động khi một nút chuột được bấm. Có ích khi quay phim màn "
"hình / thuyết trình"

#: src/effects/mousemark/metadata.json
msgctxt "Description"
msgid "Allows you to draw lines on the desktop"
msgstr "Cho phép bạn vẽ các nét trên bàn làm việc"

#: src/effects/overview/metadata.json
msgctxt "Description"
msgid "Allows you to overview virtual desktops and windows"
msgstr "Cho phép bạn xem tổng quan các bàn làm việc ảo và các cửa sổ"

#: src/effects/presentwindows/metadata.json
msgctxt "Description"
msgid "Zoom out until all opened windows can be displayed side-by-side"
msgstr "Thu nhỏ cho đến khi tất cả các cửa sổ đang mở hiển thị được cạnh nhau"

#: src/effects/resize/metadata.json
msgctxt "Description"
msgid "Resizes windows with a fast texture scale instead of updating contents"
msgstr ""
"Đổi cỡ cửa sổ với một sự đổi cỡ nhanh ở bề mặt, thay vì cập nhật nội dung"

#: src/effects/screenedge/metadata.json
msgctxt "Description"
msgid "Highlights a screen edge when approaching"
msgstr "Tô sáng cạnh màn hình khi tiến lại gần"

#: src/effects/screenshot/metadata.json
msgctxt "Description"
msgid "Helper effect for screenshot tools"
msgstr "Hiệu ứng trợ giúp cho các công cụ chụp ảnh màn hình"

#: src/effects/screentransform/metadata.json
msgctxt "Description"
msgid "Animates display transformations"
msgstr "Hoạt hoá các biến đổi hiển thị"

#: src/effects/sheet/metadata.json
msgctxt "Description"
msgid ""
"Make modal dialogs smoothly fly in and out when they are shown or hidden"
msgstr ""
"Làm các hộp thoại chế độ bay vào và ra một cách êm dịu khi chúng hiện ra và "
"biến mất"

#: src/effects/showfps/metadata.json
msgctxt "Description"
msgid "Display KWin's performance in the corner of the screen"
msgstr "Hiện hiệu suất hoạt động của KWin ở góc màn hình"

#: src/effects/showpaint/metadata.json
msgctxt "Description"
msgid "Highlight areas of the desktop that have been recently updated"
msgstr "Tô sáng những vùng bàn làm việc mới được cập nhật"

#: src/effects/slideback/metadata.json
msgctxt "Description"
msgid "Slide back windows when another window is raised"
msgstr "Trượt lùi cửa sổ khi một cửa sổ khác được nâng lên"

#: src/effects/slide/metadata.json
msgctxt "Description"
msgid "Slide desktops when switching virtual desktops"
msgstr "Trượt qua các bàn làm việc khi chuyển bàn làm việc ảo"

#: src/effects/slidingpopups/metadata.json
msgctxt "Description"
msgid "Sliding animation for Plasma popups"
msgstr "Hiệu ứng động dạng trượt cho các ô bật lên Plasma"

#: src/effects/snaphelper/metadata.json
msgctxt "Description"
msgid "Help you locate the center of the screen when moving a window"
msgstr "Giúp bạn định vị tâm của màn hình khi di chuyển cửa sổ"

#: src/effects/startupfeedback/metadata.json
msgctxt "Description"
msgid "Helper effect for startup feedback"
msgstr "Hiệu ứng trợ giúp cho phản hồi khởi động"

#: src/effects/thumbnailaside/metadata.json
msgctxt "Description"
msgid "Display window thumbnails on the edge of the screen"
msgstr "Hiển thị các hình nhỏ cửa sổ ở cạnh màn hình"

#: src/effects/touchpoints/metadata.json
msgctxt "Description"
msgid "Visualize touch points"
msgstr "Trực quan hoá các điểm chạm"

#: src/effects/trackmouse/metadata.json
msgctxt "Description"
msgid "Display a mouse cursor locating effect when activated"
msgstr "Khi được kích hoạt, hiển thị một hiệu ứng định vị con trỏ chuột"

#: src/effects/windowgeometry/metadata.json
msgctxt "Description"
msgid "Display window geometries on move/resize"
msgstr "Hiển thị dạng hình học của cửa sổ khi di chuyển / đổi cỡ"

#: src/effects/wobblywindows/metadata.json
msgctxt "Description"
msgid "Deform windows while they are moving"
msgstr "Làm méo các cửa sổ khi chúng di chuyển"

#: src/effects/zoom/metadata.json
msgctxt "Description"
msgid "Magnify the entire desktop"
msgstr "Phóng đại toàn bộ bàn làm việc"

#: src/plugins/platforms/drm/drm.json
msgctxt "Description"
msgid "Render through drm node."
msgstr "Kết xuất qua nút drm."

#: src/plugins/platforms/fbdev/fbdev.json
msgctxt "Description"
msgid "Render to framebuffer."
msgstr "Kết xuất vào framebuffer."

#: src/plugins/platforms/virtual/virtual.json
msgctxt "Description"
msgid "Render to a virtual framebuffer."
msgstr "Kết xuất vào một framebuffer ảo."

#: src/plugins/platforms/wayland/wayland.json
msgctxt "Description"
msgid "Render to a nested window on running Wayland compositor."
msgstr "Kết xuất vào một cửa sổ lồng trong trình kết hợp Wayland đang chạy."

#: src/plugins/platforms/x11/standalone/x11.json
msgctxt "Description"
msgid "Platform plugin for standalone x11 in kwin_x11."
msgstr "Phần cài cắm nền tảng cho x11 độc lập trong kwin_x11."

#: src/plugins/platforms/x11/windowed/x11.json
msgctxt "Description"
msgid "Render to a nested window on X11 windowing system."
msgstr "Kết xuất vào một cửa sổ lồng trong hệ thống cửa sổ X11."

#: src/plugins/scenes/opengl/opengl.json
msgctxt "Description"
msgid "KWin Compositor plugin rendering through OpenGL"
msgstr "Phần cài cắm trình kết hợp KWin kết xuất thông qua OpenGL"

#: src/plugins/scenes/qpainter/qpainter.json
msgctxt "Description"
msgid "KWin Compositor plugin rendering through QPainter"
msgstr "Phần cài cắm trình kết hợp KWin kết xuất thông qua QPainter"

